  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "redis": "^4.6.15",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "jest": {
      "testEnvironment": "node",
      "moduleFileExtensions": ["js"],
      "transform": {
        "^.+\\.js$": "babel-jest"
      },
      "testMatch": ["**/?(*.)+(spec|test).js"]
